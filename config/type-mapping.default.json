    "phonenumber": {
      "strategy": "VarCharText",
      "fallbackLength": 20
    },
    "phone": {
      "strategy": "VarCharText",
      "fallbackLength": 20
    },
    "url": {
      "strategy": "UnicodeText",
      "maxLengthThreshold": 2000
    },
    "password": {
      "strategy": "UnicodeText",
      "maxLengthThreshold": 2000
    },
    "username": {
      "strategy": "UnicodeText",
      "maxLengthThreshold": 2000
    },
    "identifiertext": {
      "strategy": "UnicodeText",
      "maxLengthThreshold": 2000
    },
    "guid": "uniqueidentifier",
    "uniqueidentifier": "uniqueidentifier",
    "xml": "xml"
  }
